        "laravel-echo": "^1.5.1",
        "pusher-js": "^4.3.1",
        "socket.io": "^2.2.0",
        "socket.io-client": "^2.2.0",
        "vue": "^2.5.17",
        "vue-moment": "^4.0.0",
        "vue-router": "^3.0.2",
        "vuetify": "^1.3.9",
        "vuex": "^3.0.1"
